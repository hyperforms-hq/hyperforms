{
  "name": "hyperforms-crats",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "body-parser": "^1.19.0",
    "colors": "^1.3.3",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "node-sass": "^4.12.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1",
    "typescript": "^3.5.3"
  },
  "scripts": {
    "start": "node build/server/index.js",
    "start:dev": "react-scripts start",
    "start:dev:server": "ts-node --project ./src/server/tsconfig.json ./src/server/index.ts",
    "build": "react-scripts build",
    "build:server": "tsc --project ./src/server/tsconfig.json",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "db:dump": "cross-env COMMAND=dump_db ts-node --project src/server/tsconfig.json ./tools/db-manager.ts",
    "db:restore": "cross-env COMMAND=restore_db ts-node --project src/server/tsconfig.json ./tools/db-manager.ts",
    "db:drop": "cross-env COMMAND=drop_dev_dbs ts-node --project src/server/tsconfig.json ./tools/db-manager.ts",
    "db:install": "cross-env COMMAND=create_dev_dbs ts-node --project src/server/tsconfig.json ./tools/db-manager.ts",
    "db:reinstall": "npm run db:drop && npm run db:install"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/colors": "^1.2.1",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.0",
    "@types/jest": "24.0.16",
    "@types/node": "12.6.9",
    "@types/react": "16.8.24",
    "@types/react-dom": "16.8.5",
    "cross-env": "^5.2.0",
    "husky": "^3.0.2",
    "prettier": "1.18.2",
    "pretty-quick": "^1.11.1",
    "ts-node": "^8.3.0"
  },
  "proxy": "http://localhost:5000/",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
