{
  "name": "hyperforms-crats",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@fortawesome/fontawesome-free": "^5.10.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/parcel-bundler": "^1.12.1",
    "apollo-boost": "^0.4.7",
    "apollo-client": "^2.6.8",
    "apollo-link": "^1.2.13",
    "apollo-server": "^2.9.14",
    "apollo-server-express": "^2.14.2",
    "axios": "^0.19.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "bulma": "^0.7.5",
    "classnames": "^2.2.6",
    "colors": "^1.3.3",
    "cookie-session": "^1.3.3",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "express-session": "^1.16.2",
    "final-form": "^4.18.5",
    "graphql": "^14.4.2",
    "graphql-import": "^0.7.1",
    "jest": "^24.9.0",
    "node-sass": "^4.12.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pg": "^7.3.0",
    "pg-monitor": "^1.2.1",
    "react": "^16.8.6",
    "react-apollo": "^3.1.3",
    "react-dom": "^16.8.6",
    "react-final-form": "^6.3.0",
    "react-router-dom": "^5.1.2",
    "react-table": "^7.0.0-beta.12",
    "reflect-metadata": "^0.1.10",
    "typeorm": "0.2.18"
  },
  "scripts": {
    "lint": "prettier --check './src/**/*.{ts,tsx,css,json,js,md,graphql}'",
    "fix": "prettier --write './src/**/*.{ts,tsx,css,json,js,md,graphql}'",
    "start": "cross-env NODE_ENV=production node dist-server/index.js",
    "dev": "cross-env NODE_ENV=development nodemon",
    "build": "npm-run-all build:server build:client",
    "build:server": "tsc --project tsconfig.server.json",
    "build:client": "parcel build public/index.html --public-url ./static/",
    "generate": "graphql-codegen --config codegen.yml",
    "db:dump": "cross-env COMMAND=dump_db ts-node --project tsconfig.server.json ./tools/db-manager.ts",
    "db:restore": "cross-env COMMAND=restore_db ts-node --project tsconfig.server.json ./tools/db-manager.ts",
    "db:drop": "cross-env COMMAND=drop_dev_dbs ts-node --project tsconfig.server.json ./tools/db-manager.ts",
    "db:install": "cross-env COMMAND=create_dev_dbs ts-node --project tsconfig.server.json ./tools/db-manager.ts",
    "db:reinstall": "npm run db:drop && npm run db:install",
    "test": "jest --config=jest.unit.config.js --detectOpenHandles",
    "test:ci": "jest --config=jest.ci.config.js --detectOpenHandles"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.6.1",
    "@graphql-codegen/introspection": "1.6.1",
    "@graphql-codegen/typescript": "1.6.1",
    "@graphql-codegen/typescript-operations": "^1.6.1",
    "@graphql-codegen/typescript-react-apollo": "^1.6.1",
    "@graphql-codegen/typescript-resolvers": "^1.6.1",
    "@types/classnames": "^2.2.9",
    "@types/colors": "^1.2.1",
    "@types/cookie-session": "^2.0.37",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.0",
    "@types/express-session": "^1.15.14",
    "@types/graphql": "^14.2.2",
    "@types/jest": "24.0.16",
    "@types/node": "^8.0.29",
    "@types/passport": "^1.0.0",
    "@types/passport-local": "^1.0.33",
    "@types/react": "16.8.24",
    "@types/react-dom": "16.8.5",
    "@types/react-router": "^5.1.3",
    "@types/react-router-dom": "^5.1.3",
    "concurrently": "^4.1.2",
    "cross-env": "^5.2.0",
    "husky": "^3.0.2",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "prettier": "1.19.1",
    "pretty-quick": "^2.0.1",
    "ts-jest": "^24.0.2",
    "ts-node": "3.3.0",
    "typescript": "^3.7.2"
  },
  "proxy": "http://localhost:5000/",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick"
    }
  }
}
